	<queryString language="SQL">
		<![CDATA[SELECT 
 epi_first_name,
	epi_last_name,
	eoi_doj,
	eoi_employee_id,
	eoi_official_email_id,
	cai_state,
	cai_country,
	cai_pincode,
	cai_city,
	cai_address_details,
	eoi_designation,
	epi_pan,
	ci_company_name,
	ci_company_id,
	ci_company_logo_url

FROM db_flinko.fa_company_info 
    INNER JOIN  db_flinko.fa_employee_personal_info ON 
	 db_flinko.fa_employee_personal_info.epi_company_id = db_flinko.fa_company_info.ci_company_id 
	INNER JOIN db_flinko.fa_employee_official_info ON 
	 db_flinko.fa_employee_personal_info.epi_official_id = db_flinko.fa_employee_official_info.eoi_official_id 
	INNER JOIN db_flinko.fa_company_branch_info ON 
	 db_flinko.fa_employee_official_info.eoi_office_branch_id = db_flinko.fa_company_branch_info.cbi_branch_id 
	 INNER JOIN db_flinko.fa_company_address_info ON 
     db_flinko.fa_company_address_info.cai_branch_id = db_flinko.fa_company_branch_info.cbi_branch_id 
	 where db_flinko.fa_company_info.ci_company_id=1054 and eoi_employee_id='TY07']]>
	</queryString>
